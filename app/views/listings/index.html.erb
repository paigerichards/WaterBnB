<p id="notice"><%= notice %></p>

<h1>Listings</h1>

<table>
  <thead>
    <tr>
      <th>Host</th>
      <th>Price</th>
      <th>Long</th>
      <th>Lat</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Country code</th>
      <th>Max guests</th>
      <th>Rooms</th>
      <th>Beds</th>
      <th>Shared</th>
      <th>Min nights</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.host %></td>
        <td><%= listing.price %></td>
        <td><%= listing.long %></td>
        <td><%= listing.lat %></td>
        <td><%= listing.address %></td>
        <td><%= listing.city %></td>
        <td><%= listing.state %></td>
        <td><%= listing.country_code %></td>
        <td><%= listing.max_guests %></td>
        <td><%= listing.rooms %></td>
        <td><%= listing.beds %></td>
        <td><%= listing.shared %></td>
        <td><%= listing.min_nights %></td>
        <td><%= listing.description %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
